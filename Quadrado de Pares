using System; 

class DIO {

    static void Main(string[] args) { 

      int n = int.Parse(Console.ReadLine());
            
            for(int i=1; i<=n; i++)
            {
              if (i % 2 == 0)
              {
                double b = Math.Pow(i, 2);          //escreva o seu código nos espaços em branco
                Console.WriteLine($"{i}^2 = {b}");
              }

    }
  }
}
